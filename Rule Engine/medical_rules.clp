    (assert (illness (name "hepatitis A") 
                     (symptoms "joint_pain" "vomiting" "yellowish_skin" "dark_urine" "nausea" "loss_of_appetite" "abdominal_pain" "diarrhoea" "mild_fever" "yellowing_of_eyes" "muscle_pain") 
                     (description "Hepatitis A is a highly contagious liver infection caused by the hepatitis A virus. The virus is one of several types of hepatitis viruses that cause inflammation and affect your liver's ability to function.") 
                     (precautions )))
)

(defrule Hepatitis_B
    (or (symptom (name "itching")) (symptom (name "fatigue")) (symptom (name "lethargy")) (symptom (name "yellowish_skin")) (symptom (name "dark_urine")) (symptom (name "loss_of_appetite")) (symptom (name "abdominal_pain")) (symptom (name "yellow_urine")) (symptom (name "yellowing_of_eyes")) (symptom (name "malaise")) (symptom (name "receiving_blood_transfusion")) (symptom (name "receiving_unsterile_injections")))
    =>
    (assert (illness (name "Hepatitis B") 
                     (symptoms "itching" "fatigue" "lethargy" "yellowish_skin" "dark_urine" "loss_of_appetite" "abdominal_pain" "yellow_urine" "yellowing_of_eyes" "malaise" "receiving_blood_transfusion" "receiving_unsterile_injections") 
                     (description "Hepatitis B is an infection of your liver. It can cause scarring of the organ, liver failure, and cancer. It can be fatal if it isn't treated. It's spread when people come in contact with the blood, open sores, or body fluids of someone who has the hepatitis B virus.") 
                     (precautions )))
)

(defrule Hepatitis_C
    (or (symptom (name "fatigue")) (symptom (name "yellowish_skin")) (symptom (name "nausea")) (symptom (name "loss_of_appetite")) (symptom (name "yellowing_of_eyes")) (symptom (name "family_history")))
