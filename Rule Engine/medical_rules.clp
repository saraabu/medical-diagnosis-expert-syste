(defrule Jaundice
    (or (symptom (name "itching")) (symptom (name "vomiting")) (symptom (name "fatigue")) (symptom (name "weight_loss")) (symptom (name "high_fever")) (symptom (name "yellowish_skin")) (symptom (name "dark_urine")) (symptom (name "abdominal_pain")))
    =>
    (assert (illness (name "Jaundice") 
                     (symptoms "itching" "vomiting" "fatigue" "weight_loss" "high_fever" "yellowish_skin" "dark_urine" "abdominal_pain") 
                     (description "Yellow staining of the skin and sclerae (the whites of the eyes) by abnormally high blood levels of the bile pigment bilirubin. The yellowing extends to other tissues and body fluids. Jaundice was once called the "morbus regius" (the regal disease) in the belief that only the touch of a king could cure it") 
                     (precautions )))
)

