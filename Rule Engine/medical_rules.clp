(defrule hepatitis_A
    (or (symptom (name "joint_pain")) (symptom (name "vomiting")) (symptom (name "yellowish_skin")) (symptom (name "dark_urine")) (symptom (name "nausea")) (symptom (name "loss_of_appetite")) (symptom (name "abdominal_pain")) (symptom (name "diarrhoea")) (symptom (name "mild_fever")) (symptom (name "yellowing_of_eyes")) (symptom (name "muscle_pain")))
