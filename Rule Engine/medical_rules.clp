(defrule Chronic_cholestasis
    (or (symptom (name "itching")) (symptom (name "vomiting")) (symptom (name "yellowish_skin")) (symptom (name "nausea")) (symptom (name "loss_of_appetite")) (symptom (name "abdominal_pain")) (symptom (name "yellowing_of_eyes")))
    =>
    (assert (illness (name "Chronic cholestasis") 
                     (symptoms "itching" "vomiting" "yellowish_skin" "nausea" "loss_of_appetite" "abdominal_pain" "yellowing_of_eyes") 
                     (description "Chronic cholestatic diseases, whether occurring in infancy, childhood or adulthood, are characterized by defective bile acid transport from the liver to the intestine, which is caused by primary damage to the biliary epithelium in most cases") 
                     (precautions )))
)

