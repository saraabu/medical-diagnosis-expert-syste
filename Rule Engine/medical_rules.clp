(defrule Hypothyroidism
    (or (symptom (name "fatigue")) (symptom (name "weight_gain")) (symptom (name "cold_hands_and_feets")) (symptom (name "mood_swings")) (symptom (name "lethargy")) (symptom (name "dizziness")) (symptom (name "puffy_face_and_eyes")) (symptom (name "enlarged_thyroid")) (symptom (name "brittle_nails")) (symptom (name "swollen_extremeties")) (symptom (name "depression")) (symptom (name "irritability")) (symptom (name "abnormal_menstruation")))
